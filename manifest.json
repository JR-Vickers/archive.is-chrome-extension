{
	"manifest_version": 3,
	"name": "Archive.is",
	"description": "An (unofficial) Archive.is chrome extension",
	"version": "0.1",
	"incognito": "split",
	"permissions": [
		"contextMenus",
		"activeTab",
		"scripting"
	],
	"background": {
		"service_worker": "background.js"
	},
	"icons": { "128": "images/icon.png" },
	"action": {
		"default_popup": "popup.html"
	}	
}
